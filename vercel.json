{
    "version": 2,
    "functions": {
        "src/main.ts": {
          "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}